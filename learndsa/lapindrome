	                    break;
	                case 'x': c[23]++;
	                    break;
	                case 'y': c[24]++;
	                    break;
	                case 'z': c[25]++;
	                    break;
	                
	            }
	        }
	        int temp = 0;
	        for(j=0;j<26;j++){
	            if(b[j]==c[j]){
	                temp++;
	            }
	        }
	        if(temp == 26){
	            printf("YES\n");
	        }
	        else{
	            printf("NO\n");
	        }
